<Window x:Class="wpf.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password" Height="200" Width="300">
    <StackPanel FocusManager.FocusedElement="{Binding ElementName=passwordBox}">
        <Label Margin="5px">Enter Password</Label>
        <PasswordBox Name="passwordBox" Margin="5px"></PasswordBox>
        <Button Name="savePassword" Margin="10px" Click="savePassword_Click_1" IsDefault="True" Width="60px">Enter</Button>
    </StackPanel>
</Window>
